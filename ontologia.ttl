<http://www.semanticweb.org/guill/ontologies/2024/10/ontology> rdf:type owl:Ontology ;
                                                                rdfs:comment "Esta ontologia es sobre la gestion de visitas a museos" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#boolean
xsd:boolean rdfs:comment "Indica si el grupo es una  familia" .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/artista_de_estilo
:artista_de_estilo rdf:type owl:ObjectProperty ;
                   rdfs:domain :Artista ;
                   rdfs:range :Estilo ;
                   rdfs:comment "Un artista pertenece al estilo en el que ha trabajado y ha sido reconocido" .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/creada_por
:creada_por rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :ObraDeArte ;
            rdfs:range :Artista ;
            rdfs:comment "Obra hecha por el artista" .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/exhibe_obras_de
:exhibe_obras_de rdf:type owl:ObjectProperty ;
                 rdfs:domain :SalaArtista ;
                 rdfs:range :Artista ;
                 rdfs:comment "Una SalaArtista está basada en las obras de un artista" .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/expuesta_en
:expuesta_en rdf:type owl:ObjectProperty ;
             rdfs:domain :ObraDeArte ;
             rdfs:range :Sala ;
             rdfs:comment "Se encuentra ubicado dentro de una sala" .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/obra_de_estilo
:obra_de_estilo rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :ObraDeArte ;
                rdfs:range :Estilo ;
                rdfs:comment "El estilo al que pertenece una obra" .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/parte_de
:parte_de rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Sala ;
          rdfs:range :Museo ;
          rdfs:comment "Una sala forma parte de un museo" .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/preferencia_artista
:preferencia_artista rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :Visita ;
                     rdfs:range :Estilo .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/preferencia_de_estilo
:preferencia_de_estilo rdf:type owl:ObjectProperty ;
                       rdfs:domain :Visita ;
                       rdfs:range :Estilo ;
                       rdfs:comment "Los visitantes estan interesados por un estilo concreto" .


###  http://www.semanticweb.org/guill/ontologies/2024/10/ontology/sala_sobre_estilo
:sala_sobre_estilo rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :SalaTematica ;
                   rdfs:range :Estilo ;
                   rdfs:comment "Una sala temática está basada en un estilo concreto" .


